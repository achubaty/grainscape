pages = {1205--1218},
pmid = {2184},
title = {{Landscape Connectivity: A Graph-Theoretic Perspective}},
url = {http://doi.wiley.com/10.1890/0012-9658(2001)082{\%}5B1205:LCAGTP{\%}5D2.0.CO;2},
