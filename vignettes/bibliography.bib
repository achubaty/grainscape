doi = {10.1111/j.1365-294X.2012.05587.x},
isbn = {0962-1083},
journal = {Molecular Ecology},
number = {12},
pages = {2931--2948},
title = {{Spatial genetic structure of the mountain pine beetle (Dendroctonus ponderosae) outbreak in western Canada: historical patterns and contemporary dispersal}},
volume = {21},
year = {2012}
}
